{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "dynamodb:Scan",
        "dynamodb:Query",
        "dynamodb:GetItem"
      ],
      "Resource": [
        "arn:aws:dynamodb:us-east-1:*:table/nx-tt-dev-ver3-usage-tracking",
        "arn:aws:dynamodb:us-east-1:*:table/nx-wt-prf-usage",
        "arn:aws:dynamodb:us-east-1:*:table/w1-usage",
        "arn:aws:dynamodb:us-east-1:*:table/f1-usage-two",
        "arn:aws:dynamodb:us-east-1:*:table/sedaily-column-usage",
        "arn:aws:dynamodb:us-east-1:*:table/p2-two-usage-two",
        "arn:aws:dynamodb:us-east-1:*:table/nexus-title-generator-users-prod"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    }
  ]
}
